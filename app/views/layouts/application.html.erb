<!-- header new -->
<header id="header" class="fixed-top inner">
  <div class="container-fluid d-flex">

    <div class="logo mr-auto">
      <h1 class="text-light">
        <a href="/"><%= image_tag "logo.png", class: 'search-icon', style: 'max-height:37px' %> </a></h1>
    </div>
    <nav class="nav-menu d-none d-lg-block">
      <ul>
        <li class="active"><a href="/about-us">About Us</a></li>
        <li><a href="/for-schools">For School</a></li>
        <li><a href="/edfluencers">EdFluencers</a></li>
        <li>
          <a href="#" data-toggle='modal' data-target='#newNoteModal' data-whatever='@mdo'
             onclick="changeMetaContent('Create Picnotes','description', 'Combine short notes to a cover picture and share with your friends.')">Create
            Picnotes</a></li>
      </ul>
    </nav><!-- .nav-menu -->
    <div class="header-logins">
      <%= form_tag(notes_path, method: :get, class: "form-inline search_formm") do %>
        <%= hidden_field_tag 'searchtest1', 1 %>
        <input type="text" style="padding: 25px 100px 25px 50px !important;width: 100% !important" name="search" id="search" class="form-control mr-sm-2">
        <img onclick="$('.search_formm').submit()" class="search-icon" src="/assets/search-icon_new-3a73e3281e063df7f0ea2510efd37652780b344c640b582ec98d84e4ac173d26.png">
        <div class="dropdown droptag" style="margin-top:-5px">
          <a class="btn  dropdown-toggle append-drop" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown"
             aria-haspopup="true" aria-expanded="false">Tags<span class="icon-Down_Arow"></span>
          </a>
          <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
            <a class="dropdown-item" href="javascript:void(0);">Tags</a>
            <a class="dropdown-item" href="javascript:void(0);">Person</a>
            <a class="dropdown-item" href="javascript:void(0);">Picnotes</a>
          </div>
        </div>
      <% end %>
      <% if user_signed_in? %>
        <a href="#" class="noti"><%= image_tag "noti.png" %></a>
        <form class="user mr-2">
          <div class="dropdown profile-drop">
                    <span class="profile-btn" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                      <%= image_tag current_user.avatar.url(:thumb), class: 'note-avatar-image', style: 'margin-left:0px !important' %>
                      <span class="name"> <%= @user.first_name %> <%= @user.last_name %></span>
                    </span>
            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton" style="">
              <%= link_to 'Profile', feed_index_path, class: 'dropdown-item' %>
              <a data-toggle='modal'
                 data-target='#editSessionModal'
                 data-whatever='@mdo'
                 data-dismiss="modal" class="dropdown-item" href="#">Edit Profile</a>
              <%= link_to 'Sign Out', session_path, method: :delete, class: 'dropdown-item' %>
            </div>
          </div>
        </form>
      <% else %>
        <button type="button" class='btn-login' data-toggle='modal' data-target='#newSessionModal' data-whatever='@mdo'>Login</button>

        <button type="button" class='btn-signup' data-toggle='modal' data-target='#newUserModal' data-whatever='@mdo' onclick="changeMetaContent('Sign Up','description', 'Connect with Facebook or Sign Up with Email')">Sign
          Up
        </button>
        </div>
      <% end %>
      <button type="button" class="mobile-nav-toggle d-lg-none"><span class="icon-open-menu"></span></button>
      </div>

</header><!-- End Header -->
