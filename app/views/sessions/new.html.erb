<h1 class = "hello" style = 'margin-top: 20px;'> Sign In </h1>

<div class = "creation_form">
<%= form_for :session, remote: true, url: session_path do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  <%=f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit class: 'no-look-btn'%>
<% end %>
</div>
