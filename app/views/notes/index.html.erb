<div style = 'background-color: white; z-index: 0; height: 100vw;'>
<%= render './partials/user_dashboard' %>

<div id='note-content'></div>

<% if @test %>
  <div style = 'text-align: center; margin-bottom: 20px;'>
    <p> Results for #<%= @test %> </p>
  </div>
<% end %>

<div class = "notes-container">
  <% @notes.each do |note| %>

    <div class = 'note-container'>
      <%= link_to(note_path(note), remote: true) do %>
        <%= image_tag note.image.url(:thumb) if note.image.present? %>
        <div class = 'note-container-text'>
          <p class = "no-look-btn"> <%= truncate(note.title, length: 18) %> </p>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
</div>
