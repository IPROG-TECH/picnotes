<script>
        var title = '';
        var url = '<%="users/"+@user.id.to_s+"-"+@user.username.parameterize%>';
        ChangeUrlForUsers(title, url)
</script>

<%= render './partials/other_user_dashboard' %>

<div id='note-content'></div>

<div class = 'flex-center'>
  <div class = "notes-container">
    <% @user.notes.each do |note| %>

    <div class = 'note-container' onclick='ChangeUrl("", "<%="notes/"+note.id.to_s+"-"+note.title.parameterize%>")'>
      <%= link_to(note_path(note), remote: true) do %>
        <%= image_tag note.image.url(:thumb) if note.image.present? %>
        <div class = 'note-container-text'>
          <p class = "mobile-font note-btn-font"> <%= truncate(note.title, length: 25) %> </p>
        </div>
      <% end %>
    </div>
    <% end %>
  </div>
</div>
